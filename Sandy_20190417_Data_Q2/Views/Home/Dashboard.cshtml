@model IEnumerable<Sandy_20190417_Data_Q2.Models.Data>

@{
    ViewBag.Title = "Dashboard";
}


<h2>Dashboard</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ticket)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResponseMinutes)
        </th>
        <th></th>

    </tr>

    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>

                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>

                    @Html.DisplayFor(modelItem => item.Ticket)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ResponseMinutes)
                </td>
            </tr>
        }



    }


    <tr></tr>

    
    
     
    <tr>
        <td></td>

        <td>Ticket:@Model.Sum(Model => Model.Ticket)</td>
        <td>AvgResponseMinutes:@Model.Average(Model => Model.ResponseMinutes) Minutes ( @ViewBag.Day Day       /  @ViewBag.Hour Hour )</td>

    </tr>
</table>




